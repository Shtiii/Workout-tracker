{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/service-worker.js",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/manifest.json",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "headers": {
        "X-Frame-Options": "DENY",
        "X-Content-Type-Options": "nosniff",
        "Referrer-Policy": "origin-when-cross-origin",
        "Permissions-Policy": "camera=(), microphone=(), geolocation=()"
      }
    }
  ],
  "env": {
    "NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN": "workout-tracker-c7d57.firebaseapp.com",
    "NEXT_PUBLIC_FIREBASE_PROJECT_ID": "workout-tracker-c7d57",
    "NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET": "workout-tracker-c7d57.appspot.com"
  }
}
